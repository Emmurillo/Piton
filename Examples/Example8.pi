defina factorial (x):
    si x == 0 :
        retorne 1;
    ;
    resp = 1;
    cont = 1;
    mientras x <= x:        #ERROR: nunca sale del ciclo
        resp *= cont;
        cont += 1;
    ;
    retorne resp;
;


# Sección del código principal 

x = 5;
muestre "El factorial de " + x;
x = factorial(5);
imprima " es " + x;
